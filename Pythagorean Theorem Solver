#include <iostream>
using namespace std;

    int main() {
    int a, b;

    cout << "Insert the value of your A: ";
    cin >> a;

    cout << "Insert the value of your B: ";
    cin >> b;

    int atwo = (a*a ), btwo = ( b*b ), ctwo = ( atwo+btwo );

    cout << "C squared is: " << ctwo << endl;
    int c = sqrt(ctwo);

    cout << "The value of your C is: " << c;

}
